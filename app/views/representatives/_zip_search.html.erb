<%= form_tag '#', class: 'form-horizontal' do %>
  <div class="control-group">
    <%= label_tag 'zip', "What's your zip code?", class: "control-label" %>
    <div class="controls">
      <%= text_field_tag :zip %>
      <%= submit_tag "Find", class: 'btn', id: 'zip-find-button' %>
    </div>
  </div>
<% end %>

